$ASSIGN =
$COUNT #
$MAX_FREQ_STRING maxfreqstring
$OPEN_PARENS \(
$CLOSE_PARENS \)
$GRTNOT >!
$PRINT print
$COMMA ,
$FIND find
$DIFF diff
$UNION union
$INTERS intersec
$REGEX \'($NONSINGLEQUOTE|$ESCAPED_SINGLE_QUOTE)*\'

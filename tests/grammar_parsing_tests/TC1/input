$DIGIT [0-9]
$IDENTIFIER_CHARACTER ($LETTER|$DIGIT|_|-)
$ASCII [\ -~]
$NONQUOTE [^"] IN $ASCII
$NONSINGLEQUOTE [^'] IN $ASCII
$ESCAPED_QUOTE \\\"
$ESCAPED_SINGLE_QUOTE \\\'

$NEW_LINE \

$EPSILON 

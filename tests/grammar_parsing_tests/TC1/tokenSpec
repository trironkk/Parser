$LETTER [a-zA-Z]
$DIGIT [0-9]
$IDENTIFIER_CHARACTER ($LETTER|$DIGIT|_|-|\')
$ALL (\	|\
|\"|#|!|&|\'|\$|%|\*|\+|\(|\)|\.|/|,|-|3|2|1|0|7|6|5|4|;|:|9|8|\?|>|=|<|D|E|F|G|@|A|B|C|L|M|N|O|H|I|J|K|U|T|W|V|Q|P|S|R|\]|\||_|^|Y|X|\[|Z|f|g|d|e|b|c|a|n|o|l|m|j|k|h|i|w|v|u|t|s|r|q|p|~|}|{|z|y|x)
$ALL_BUT_ESCAPE [^\\] IN $ALL
$ESCAPED_SPACE \\\ 
$ESCAPED_QUOTE \\\" 
$ESCAPED_SINGLE_QUOTE \\\'
$ESCAPED_BACK_SLASH \\
$ESCAPED_CLOSE_PARENS \\\)
$ESCAPED_OPEN_PARENS \\\(
$ESCAPED_NEW_LINE \\ $NEW_LINE

$NEW_LINE \

$CLS_CHAR ($LETTER|$DIGIT|$ESCAPED_SPACE|$ESCAPED_QUOTE|$ESCAPED_SINGLE_QUOTE|$ESCAPED_NEW_LINE|$ESCAPED_BACK_SLASH|~|\"|\')
$RE_CHAR ($LETTER|$DIGIT|$ESCAPED_SPACE|$ESCAPED_QUOTE|$ESCAPED_SINGLE_QUOTE|$ESCAPED_NEW_LINE|$ESCAPED_BACK_SLASH|_|-|;|=|#|$ESCAPED_CLOSE_PARENS|$ESCAPED_OPEN_PARENS|>|!|,)
$DASH -
$DOT \.
$PLUS \+
$CARROT ^
$CLOSE_BRACKET \]
$OPEN_BRACKET \[
$CLOSE_PARENS \)
$OPEN_PARENS \(
$IN IN
$STAR \*
$EPSILON  
$UNION \|
$RULE_ASSIGN ::=
$RULE < ($IDENTIFIER_CHARACTER)* >
$TOKEN_TYPE \$ ($IDENTIFIER_CHARACTER) +
$DEFINED_CLASS $TOKEN_TYPE
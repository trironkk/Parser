$OPERATORS (\*|\+|-|%|<|>|=|^)
$SPECIAL (\?|#|\$|{|}|;|@)
$DIGIT [0-9]
$LETTER [A-Za-z]
$ANY (\ |\*|$LETTER|$DIGIT|$SPECIAL|$OPERATORS|\'|$TAB|$DOT|\\|/|$COLON|$OPEN_BRACKET|$CLOSE_BRACKET)
$TAB \	
$QUOTE \"
$NEW_LINE \


$DOT \.
$CLASS class
$PUBLIC public
$PRIVATE private
$INT int
$STRING String
$BOOLEAN boolean
$DOUBLE double
$OPEN_PARENS \(
$CLOSE_PARENS \)
$OPEN_BRACKET \[
$CLOSE_BRACKET \]
$COMMA ,
$COLON :
$IDENTIFIER ($LETTER|_) ($LETTER|$DIGIT|_)*
$COMPARATORS $OPERATORS
$LITERALS $DIGIT($DIGIT)*
$SEPARATORS $SPECIAL($SPECIAL)*
$LINE_COMMENT (/)(/)($ANY)*
$BLOCK_COMMENT (/)(\*)($ANY|$NEW_LINE)*(\*)(/)
$LITERAL_STRING (\")($ANY)*(\")
$LITERAL_INDEX (\[) ($DIGIT)* (\])
$IDENTIFIER_INDEX (\[) $IDENTIFIER (\])